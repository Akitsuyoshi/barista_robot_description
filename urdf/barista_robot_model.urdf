<?xml version="1.0"?>
<robot name="barista_robot">

  <!-- Base Link -->
  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0.155" radius="0.178" />
      </geometry>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.155" radius="0.178" />
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.004961541667" ixy="0" ixz="0" iyy="0.004961541667" iyz="0" izz="0.007921"/>
    </inertial>
  </link>

  <!-- Wheel Left -->
  <link name="left_wheel">      
    <visual>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.0206" radius="0.0352"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.0206" radius="0.0352"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <mass value="0.05"/>
      <inertia ixx="0.00001725616667" ixy="0" ixz="0" iyy="0.00001725616667" iyz="0" izz="0.000030976"/>
    </inertial>
  </link>

  <joint name="joint_left_wheel" type="continuous">
    <origin rpy="0 0 0" xyz="0 0.15 -0.0573"/>
    <child link="left_wheel"/>
    <parent link="base_link"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>

  <!-- Wheel Right -->
  <link name="right_wheel">      
    <visual>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.0206" radius="0.0352"/>
